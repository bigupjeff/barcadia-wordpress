{
    "env": {
        "browser": true,
        "es2021": true
    },
	"parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
	"settings": {
		"react": {
		  	"version": "detect"
		}
	},
	"plugins": [
        "react",
		"@typescript-eslint",
		"prettier"
    ],
    "extends": [ 
		"eslint:recommended",
		"plugin:react/recommended",
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
    "rules": {
    }
}
